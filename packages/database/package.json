{
  "name": "database",
  "version": "0.0.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "dev:db:docs:serve": "npx prisma-docs-generator serve",
    "dev:db:studio": "dotenv -e .env.development --no-expand -- npx prisma studio",
    "dev:db:seed": "dotenv -e .env.development --no-expand -- npx prisma db seed",
    "dev:db:generate": "dotenv -e .env.development --no-expand -- npx prisma generate",
    "dev:db:push": "dotenv -e .env.development --no-expand -- npx prisma db push --skip-generate",
    "dev:studio": "dotenv -e .env.demo --no-expand -- npx prisma studio",
    "db:seed": "dotenv -e .env.demo --no-expand -- npx prisma db seed",
    "db:generate": "dotenv -e .env.demo --no-expand -- npx prisma generate",
    "db:push": "dotenv -e .env.demo --no-expand -- npx prisma db push --skip-generate"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/bcrypt": "^5.0.1",
    "@types/node": "^20.8.9",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "eslint-config-custom": "workspace:*",
    "prisma": "5.5.2",
    "prisma-dbml-generator": "^0.10.0",
    "prisma-docs-generator": "^0.8.0",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^5.2.2",
    "zod": "^3.22.4",
    "zod-prisma-types": "^2.8.1"
  }
}
